package Is
  import Equal as Equal
  import Equal (Equal)
  import Bool as Bool
  import Bool (Bool, true, false)
where

def Is (a: Bool): Type = Equal Bool true a
def is: Is(true)       = Equal.refl Bool true

def Isnt (a: Bool): Type = Equal Bool false a
def isnt: Isnt(false) = Equal.refl Bool false
